
<% extend('../layout') %>
<% users.forEach(function (user) { %>
    <li>
        <h1><%= user.name %></h1>
    </li>
<% }) %>
<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">name</th>
      <th scope="col">email</th>
      <th scope="col">action</th>
    </tr>
  </thead
  <tbody id="tbody">
    <tr>
      <th scope="row">Default</th>
       <td>Column content</td>
       <td>Column content</td>
       <td>Column content</td>
    </tr>
  </tbody>

</table>
